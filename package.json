{
  "name": "rain-ui-components",
  "private": true,
  "version": "0.0.1",
  "description": "Reusable UI components for all Rain Frontend projects.",
  "type": "module",
  "main": "./dist/rain-ui-components.umd.js",
  "module": "./dist/rain-ui-components.es.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/rain-ui-components.es.js",
      "require": "./dist/rain-ui-components.umd.js"
    }
  },
  "keywords": [
    "components",
    "component",
    "framework",
    "ui-library",
    "ui"
  ],
  "scripts": {
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "build": "vite build",
    "deploy": "yarn cache clean && yarn install --network-concurrency 1 && yarn build",
    "dev": "vite",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^2.0.1",
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "sass": "^1.50.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "svelte-windicss-preprocess": "^4.2.6",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-windicss": "^1.8.4",
    "windicss": "^3.5.1"
  },
  "dependencies": {}
}
